                <?tgb_sheet pic_width="2500" pic_height="1768" color="FFFFFF" dpi="235" grid="0" step="10" gstep="10" grid_color="808080" snap="0" show_fns="1" scale="100" basescale="100" width="1021" height="722" min_x="0" min_y="0" imagescale="41" imagescaledpi="1" icn="ICN-URALM-A-A71000-A-00000-00095-A-01-1" icn_footer="1" woborder="0" crc32="3654164173"?>
