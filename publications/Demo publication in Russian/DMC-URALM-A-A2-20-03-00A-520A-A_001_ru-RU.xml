              <?tgb_illustration showname="1" printprops="2" browseprops="2" legviewmode="3" legflags="2" callouts="0" icn="" icn_footer="0" papersize="0" objectflags="0" image_width="0" overlay_width="642" applicrefid="" illnumber="5"?>
